// npx prisma migrate dev --name create_users_table
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              String  @id @default(cuid())
  firstName       String
  lastName        String?
  email           String  @unique
  password        String
  salt            String
  profileImageURL String?
}
